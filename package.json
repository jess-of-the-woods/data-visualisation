{
  "name": "data-visualisation",
  "version": "0.0.0",
  "description": "",
  "main": "server.js",
  "browser": "src/index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "run-serve": "nodemon ./server.js",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jess-of-the-woods/data-visualisation.git"
  },
  "author": "jess-of-the-woods",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jess-of-the-woods/data-visualisation/issues"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "express": "^4.13.4",
    "react-dom": "^15.0.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "budo": "^8.2.2",
    "cheerio": "^0.20.0",
    "cors": "^2.7.1",
    "d3": "^3.5.16",
    "domready": "^1.0.8",
    "dotenv": "^2.0.0",
    "jquery": "^2.2.3",
    "promise": "^7.1.1",
    "react": "^15.0.2",
    "superagent": "^1.8.3",
    "twitter": "^1.2.5"
  }
}
